<%= form_for @changeset, @action, fn f -> %>
  <%= if @changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>
  <%= label f, :user_id, "email" %>
  <%= select f, :user_id, Enum.map(TaskTracker.Users.list_users_by_supervisor(@current_user), &{&1.email, &1.id}) %>
  <%= error_tag f, :user_id %>
  <div>
  <%= submit "Save" %>
  </div>
<% end %>
